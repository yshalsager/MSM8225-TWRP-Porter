/system       ext4  /dev/block/mmcblk0p21
/cache        ext4  /dev/block/mmcblk0p22
/data         ext4  /dev/block/mmcblk0p24
/preload      ext4  /dev/block/mmcblk0p23  flags=display="Preload";backup=1
/persist      ext4  /dev/block/mmcblk0p12  flags=display="Persist";backup=1
/efs          ext4  /dev/block/mmcblk0p16  flags=display="EFS";backup=1
/boot         emmc  /dev/block/mmcblk0p8
/recovery     emmc  /dev/block/mmcblk0p13
/external_sd  vfat  /dev/block/mmcblk1p1   flags=display="Micro SD";storage;wipeingui;removable
